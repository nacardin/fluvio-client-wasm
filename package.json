{
  "author": "Infinyon Contributors <team@infiyon.com>",
  "name": "fluvio-client-wasm",
  "version": "0.1.0",
  "description": "WASM bindings for Fluvio distributed stream client",
  "license": "Apache-2.0",
  "keywords": [
    "fluvio",
    "stream",
    "wasm",
    "client"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infinyon/fluvio-client-wasm.git"
  },
  "homepage": "https://fluvio.io",
  "main": "dist/fluvio-client.js",
  "types": "dist/fluvio-client.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "rimraf dist && rollup --config",
    "start": "rollup -c rollup.config.js --watch",
    "prepack": "rollup --config",
    "webpack-dev": "rimraf dist pkg && webpack-dev-server --open -d",
    "test": "cargo test && wasm-pack test --headless"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.1",
    "@wasm-tool/rollup-plugin-rust": "^1.0.6",
    "@wasm-tool/wasm-pack-plugin": "^1.1.0",
    "copy-webpack-plugin": "^5.0.3",
    "rimraf": "^3.0.0",
    "rollup": "^2.50.2",
    "rollup-plugin-dts": "3",
    "tslib": "^2.2.0",
    "typescript": "4",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.3",
    "webpack-dev-server": "^3.7.2"
  }
}
